{
    "name": "acegen-with-server",
    "displayName": "AceGen",
    "description": "acegen with server",
    "version": "0.0.30",
    "publisher": "Annette",
    "license": "EPL-1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/itemis/xtext-languageserver-example.git"
    },
    "bugs": {
        "url": "https://github.com/itemis/xtext-languageserver-example/issues"
    },
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:acegen"
    ],
    "main": "out/extension",
    "contributes": {
        "languages": [
            {
                "id": "acegen",
                "aliases": [
                    "acegen"
                ],
                "extensions": [
                    ".ace"
                ],
                "configuration": "./acegen.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "acegen",
                "scopeName": "text.acegen",
                "path": "./syntaxes/acegen.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^17.0.18",
        "@types/vscode": "^1.64.0",
        "typescript": "^4.5.5",
        "vscode-test": "^1.5.2"
    },
    "dependencies": {
        "vscode-languageclient": "^7.0.0"
    },
    "scripts": {
        "prepublish": "tsc -p ./src",
        "compile": "tsc -p ./src",
        "watch": "tsc -w -p ./src",
        "update-vscode": "node ./node_modules/vscode/bin/install"
    }
}
